{
  "app": {
    "version": "sunmao/v1",
    "kind": "Application",
    "metadata": {
      "name": "some App"
    },
    "spec": {
      "components": [
        {
          "id": "text",
          "type": "core/v1/text",
          "properties": {
            "value": {
              "raw": "text",
              "format": "plain"
            }
          },
          "traits": [
            {
              "type": "test/v1/timeout",
              "properties": {
                "value": "{{input.value + Math.random()}}"
              }
            }
          ]
        },
        {
          "id": "input",
          "type": "test/v1/input",
          "properties": {
            "defaultValue": "foo"
          },
          "traits": []
        },
        {
          "id": "tester",
          "type": "test/v1/tester",
          "properties": {
            "text": "{{text.result}}"
          },
          "traits": []
        }
      ]
    }
  }
}
